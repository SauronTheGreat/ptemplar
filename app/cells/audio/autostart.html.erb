<script src="/assets/pslidejs/jwplayer.js" type="text/javascript"></script>

<div id="container" style="display: none;">Loading the player ...</div>
<script type="text/javascript">
    jwplayer("container").setup({
        flashplayer:"/assets/player.swf",
        file:"<%= @pdata.audios.first.audio.url %>",
        height:<%= 1 %>,
        width:<%= 1 %>,
        autostart:true,
        screencolor:"transparent"
    });
</script>



