<script src="/assets/pslidejs/jwplayer.js" type="text/javascript"></script>

<div id="container">Loading the player ...</div>
<script type="text/javascript">
    jwplayer("container").setup({
        flashplayer:"/assets/player.swf",
        file:"<%= @pdata.videos.first.video.url %>",
        height:<%= 0.9*@height %>,
        width:<%= 0.9*@width %>,
        autostart:'true'
    });
</script>



